# 数据库配置（连接到现有的 TeslaMate 数据库）
TESLAMATE_DB_HOST=localhost        # TeslaMate 数据库主机地址
TESLAMATE_DB_PORT=5432            # 数据库端口
TESLAMATE_DB_USER=teslamate        # 数据库用户名
TESLAMATE_DB_PASSWORD=teslamate    # 数据库密码
TESLAMATE_DB_NAME=teslamate        # 数据库名称
TESLAMATE_DB_SSLMODE=disable        # SSL 模式：disable, require, verify-ca, verify-full

# 应用配置
CYBERUI_SERVER_HOST=0.0.0.0
CYBERUI_SERVER_PORT=8080
CYBERUI_SERVER_MODE=release

# 日志配置
LOG_LEVEL=info
